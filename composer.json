        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/di": "^3.0@dev",
        "yiisoft/yii-core": "^3.0@dev"
    },
    "require-dev": {
        "psr/log": "~1.1.0",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/db-sqlite": "^3.0@dev",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/db-pgsql": "^3.0@dev",
        "phpunit/phpunit": "^7.3",
        "friendsofphp/php-cs-fixer": "^2.2",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "suggest": {
        "yiisoft/db": "^3.0 for data base"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Rbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii\\log\\tests\\": "vendor/yiisoft/log/tests",
            "Yiisoft\\Rbac\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "tests": "config/tests.php"
        }
    }
}
